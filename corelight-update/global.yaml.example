verbose: false
exp_features: false
webserver:
    enable: false
    tls: true
    tls_cert: /etc/corelight-update/global/cert.crt
    tls_key: /etc/corelight-update/global/cert.key
    port: 8443
process_feeds: true
interval_minutes: 60
geoip:
    enabled: false
    interval_hours: 0
    account_id: 0
    license_key: ""
    database_directory: /usr/share/GeoIP
remote_global_conf_files: []
parallel_push_limit: 10
auto_update_policies:
    enable: true
    filename: db-config.yaml
